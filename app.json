{
  "expo": {
    "name": "native_metal",
    "slug": "native_metal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/metal.png",
    "splash": {
      "image": "./assets/metal.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6310C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/metal.png",
        "backgroundColor": "#F6310C"
      }
    },
    "web": {
      "favicon": "./assets/metal.png"
    }
  }
}
